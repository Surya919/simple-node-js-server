{
  "name": "simple-nodejs-server",
  "version": "0.0.1",
  "description": "Simple node.js server playground",
  "main": "index.js",
  "engines": {
    "node": "7.x",
    "npm": "4.x"
  },
  "keywords": [
    "node.js",
    "node",
    "javascript",
    "server",
    "gulp",
    "babel"
  ],
  "scripts": {
    "build": "babel src -d dist",
    "start": "nodemon src/index.js --exec babel-node",
    "serve": "node dist/index.js",
    "test": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexdevero/Simple-Node.js-Server.git"
  },
  "author": {
    "name": "Alex Devero",
