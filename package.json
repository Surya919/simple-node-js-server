{
  "name": "simple-node-js-server",
  "version": "0.0.2",
  "description": "Simple node.js server ready to use",
  "main": "index.js",
  "engines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=0.24.x"
  },
  "keywords": [
    "node.js",
    "node",
    "javascript",
    "server",
    "gulp",
    "babel"
  ],
  "scripts": {
    "build": "babel src -d dist",
    "start": "nodemon src/index.js --exec babel-node",
    "serve": "node dist/index.js",
    "test": "mocha --require babel-register --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexdevero/Simple-Node.js-Server.git"
  },
  "author": {
    "name": "Alex Devero",
    "url": "https://github.com/alexdevero"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexdevero/Simple-Node.js-Server/issues"
  },
  "homepage": "https://github.com/alexdevero/Simple-Node.js-Server#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.14.3"
  }
}
